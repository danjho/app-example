{
    "dart.flutterSdkPath": "~/fvm/default",
    "editor.rulers": [
        80,
        120
    ],
    "workbench.colorTheme": "One Dark Pro Night Flat",
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.selectionHighlight": false,
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    },
    "workbench.iconTheme": "symbols",
    "editor.minimap.enabled": false,
    "workbench.activityBar.location": "hidden",
    "workbench.statusBar.visible": false,
    // "window.titleBarStyle": "native",
    "editor.renderLineHighlight": "gutter",
    "window.customTitleBarVisibility": "never",
    "editor.stickyScroll.enabled": false,
    "notebook.stickyScroll.enabled": false,
    "explorer.compactFolders": false,
    "terminal.integrated.stickyScroll.enabled": false,
    "workbench.tree.enableStickyScroll": false,
    "explorer.confirmDelete": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "explorer.confirmDragAndDrop": false,
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "breadcrumbs.enabled": false,
    "chat.commandCenter.enabled": false,
    "geminicodeassist.displayInlineContextHint": false,
    "workbench.colorCustomizations": {
        "editor.background": "#0f0f0f",
        "sideBar.background": "#0f0f0f",
        "window.activeBorder": "#0f0f0f",
        "activityBar.background": "#0f0f0f",
        "titleBar.activeBackground": "#0f0f0f",
        "statusBar.background": "#0f0f0f",
        "terminal.background": "#0f0f0f",
        "panel.background": "#0f0f0f",
        "panel.border": "#0f0f0f",
        "tab.border": "#0f0f0f",
        "sideBar.border": "#0f0f0f",
        "editorGroup.border": "#0f0f0f",
        "sideBarSectionHeader.border": "#0f0f0f",
        "editorGroupHeader.tabsBorder": "#0f0f0f",
        "tab.activeBackground": "#0f0f0f",
        "tab.inactiveBackground": "#0f0f0f",
        "tab.background": "#0f0f0f",
        "statusBar.border": "#0f0f0f",
        "activityBar.border": "#0f0f0f",
        "statusBarItem.hoverBackground": "#0f0f0f",
        "input.background": "#0f0f0f",
        "input.border": "#0f0f0f",
        "titleBar.inactiveBackground": "#0f0f0f",
        "editorGroupHeader.tabsBackground": "#0f0f0f",
        "editorIndentGuide.background1": "#212121",
        "editorIndentGuide.background2": "#212121",
        "editorIndentGuide.background3": "#212121",
        "editorIndentGuide.background4": "#212121",
        "editorIndentGuide.background5": "#212121",
        "editorIndentGuide.background6": "#212121",
        "editorRuler.foreground": "#181818",
        "editorIndentGuide.activeBackground1": "#4c4c4c",
        "editorIndentGuide.activeBackground2": "#4c4c4c",
        "editorIndentGuide.activeBackground3": "#4c4c4c",
        "editorIndentGuide.activeBackground4": "#4c4c4c",
        "editorIndentGuide.activeBackground5": "#4c4c4c",
        "editorIndentGuide.activeBackground6": "#4c4c4c",
    },
    "editor.tokenColorCustomizations": {
        // "comments": "#6A8AD977",
